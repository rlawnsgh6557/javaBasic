		//5명의 3과목 점수 선언 jum[][]
		int[][] jum = {{100,80,90},{70,90,80},{60,90,80},{90,80,70},{40,50,60}};
		int hap = 0;
		float py = 0.0f;
		int[] tot;
		float[] avg;
		//전체총점(hap), 전체평균,(py) 과목총점(tot), 과목평균(avg),
		/*
		 성명		국어		영어		수학
		 
		 
		 
		 */
		System.out.println("names"+names);
	}
}