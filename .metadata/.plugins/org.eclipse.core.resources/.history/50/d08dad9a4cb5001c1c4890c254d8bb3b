package sec2;

public class DmbPhoneEx {

	public static void main(String[] args) {
		DmbPhone dmb1 = new DmbPhone("S22","Black",24);
		System.out.println("모델명 : "+dmb1.model);
		System.out.println("색상 : "+dmb1.color);
		System.out.println("채널 : "+dmb1.channel);
		dmb1.powerOn();
		dmb1.bell();
		dmb1.sendVoice("여보세요");
	}

}
