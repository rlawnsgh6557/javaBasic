package sec2;

public class CellPhone {
	String model;
	String color;
	
	public void powerOn(){
		System.out.println("전원을 켭니다.");
	}
	public void powerOff(){
		System.out.println("전원을 끕니다.");
	}
	public void bell(){
		System.out.println("벨이 울립니다.");
	}
	public void sendVoice(String sendMessage){
		System.out.println("전달한 메시지를 출력");
	}
	public void receiveVoice(String toMessage){
		System.out.println("전달받은 메시지를 출력");
	}
	public void hangUp(){
		System.out.println("전화를 끊습니다.");
	}
}
