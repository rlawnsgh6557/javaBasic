package sec2;

public class SupersonicAirplane extends AirPlane{
	public static final int NORMAL = 1;
	public static final int SUPERSONIC = 2;
	
	public int flyMode = NORMAL;

	@Override
	public void fly() {
		if(flyMode==2) {
			System.out.println("초음속비행을합니다.");
		} else {
			System.out.println("일반비행을 합니다.");
		}
		super.fly();
	}

	
}
